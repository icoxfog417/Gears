<%@ Master Language="VB" CodeFile="pppMaster.master.vb" Inherits="pppMaster" %>

<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN""http://www.w3.org/TR/html4/strict.dtd">

<html>
<head runat="server">
    <!-- アイコン用 -->
    <link rel="shortcut icon" href="favicon.ico" type="image/vnd.microsoft.icon" />
    <link rel="icon" href="favicon.ico" type="image/vnd.microsoft.icon" />

    <link type="text/css" href="./css/pppStyle.css" rel="stylesheet" />
    <link type="text/css" href="./css/gearsStyle.css" rel="stylesheet" />
    <link type="text/css" href="./css/custom-theme/jquery-ui.custom.css" rel="stylesheet" />

    <script src="./lib/jQuery.js" type="text/javascript"></script>
    <script src="./lib/jquery-ui.custom.min.js" type="text/javascript"></script>
    <script src="./lib/jQuery/jquery.bgiframe.min.js" type="text/javascript"></script>
    <script src="./lib/gears-script.js" type="text/javascript"></script>
    <script src="./lib/pppScript.js" type="text/javascript"></script>

    <style>
        .document-body
        {
            width:900px;
            margin:0px 0px 0px 15px;
            
            }
        h3.caption
        {
            border-left:7px solid #ccc;  
            border-bottom:1px solid #ccc;  
            padding:.6em .8em 
            }
        table.ppp-table
        {
            border-collapse:collapse;
            }
        table.ppp-table th
        {
            background-color:tan;
            color:White;
            }
        table.ppp-table th,table.ppp-table td
        {
            border:2px solid wheat;
            
            }
    </style>

    <asp:ContentPlaceHolder id="pppHead" runat="server">
    </asp:ContentPlaceHolder>
</head>
<body class="ppp-apl">
    <form id="form1" runat="server">

    <asp:ScriptManager ID="tsmMaster" runat="server">
    </asp:ScriptManager>
    <asp:ContentPlaceHolder id="pppPreDeclare" runat="server">
    </asp:ContentPlaceHolder>

    <!-- メニュー直下の位置に配置するコンテンツ。ヘルプのリンクなどがあればここに記載 -->
    <asp:Panel ID="pnlOptionMenu" runat="server" CssClass="ppp-menu-option">
        <asp:Panel ID="pnlPageTitle" runat="server" CssClass="ppp-caption title">
            <!-- IE6でなければテーブルを使用する必要はないのだが・・・　-->
            <table width="100%">
                <tr>
                <td><asp:ContentPlaceHolder id="pppPageTitle" runat="server"></asp:ContentPlaceHolder>
                </td>
                <td align="right">
                    <asp:ContentPlaceHolder id="pppOptionMenu" runat="server"></asp:ContentPlaceHolder>
                </td>
                </tr>
            </table> 
        </asp:Panel>
    </asp:Panel>

    <div>
        <asp:ContentPlaceHolder id="pppHeader" runat="server">
        </asp:ContentPlaceHolder>
    </div>

    <div>
        <asp:ContentPlaceHolder id="pppContent" runat="server">
        </asp:ContentPlaceHolder>
    </div>

    <div>
        <asp:ContentPlaceHolder id="pppFooter" runat="server">        
        </asp:ContentPlaceHolder>
    </div>


    <asp:ContentPlaceHolder id="pppEndDeclare" runat="server">
    </asp:ContentPlaceHolder>

    </form>
</body>
</html>
